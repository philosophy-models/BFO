        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#Prudence</IRI>
        <Literal xml:lang="en">Prudence</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#Qualified</IRI>
        <Literal xml:lang="en">“A dog as an object with certain qualities: bad breath, soft fur, dotted; can be qualified commonly as ‘dog’ or ‘furry’, or peculiarly as ‘Fido’.” (Wyss)</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
