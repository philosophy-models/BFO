        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#Courage</IRI>
        <Literal xml:lang="en">Prudence, wisdom, justice, courage and moderation are virtues</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#Courage</IRI>
        <Literal xml:lang="en">Courage</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#Disposed</IRI>
        <Literal xml:lang="en">A dog as a further differentiated qualified thing: as running, barking, brave. (Wyss)</Literal>
    </AnnotationAssertion>
