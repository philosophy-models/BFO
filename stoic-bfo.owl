        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#SimpleAssertible</IRI>
        <Literal xml:lang="en">Simple assertibles include propositions like: “it is cold”; “it is raining this morning” and “no one is running.”</Literal>
